# Core
OPENAI_API_KEY=sk-...

# Optional: fast OHLCV provider (Birdeye)
BIRDEYE_API_KEY=
FAST_OHLCV_PROVIDER=birdeye

# UI / Server
# Local default: 3013 (npm run start); override with --port or TOKEN_AI_UI_PORT
TOKEN_AI_UI_PORT=3013
TOKEN_AI_DEMO_MODE=1
TOKEN_AI_BROADCAST_CHILD_LOGS=1

# MCP (Streamable HTTP)
# Local default: 3930; point UI proxy to this if using the browser proxy
TOKEN_AI_MCP_PORT=3930
TOKEN_AI_MCP_TOKEN=
TOKEN_AI_MCP_PUBLIC_URL=http://127.0.0.1:3930/mcp
TOKEN_AI_MCP_OAUTH=true

# OAuth / OIDC (for production; demo mode relaxes requirements)
# TOKEN_AI_OIDC_AUTHORIZATION_ENDPOINT=
# TOKEN_AI_OIDC_TOKEN_ENDPOINT=
# TOKEN_AI_OIDC_USERINFO=
# TOKEN_AI_OIDC_CLIENT_ID=
MCP_USER_JWT_SECRET=

# Supabase (optional: enable magic-link login in browser UI)
SUPABASE_URL=
SUPABASE_ANON_KEY=

# Database (temporary â€” Prisma; can point to Supabase Postgres)
# Leave empty to run without DB-backed features
DATABASE_URL=

# Realtime Voice (optional)
TOKEN_AI_REALTIME_MODEL=gpt-realtime
TOKEN_AI_REALTIME_VOICE=verse

# Events auth for UI POSTs (optional but recommended)
TOKEN_AI_EVENTS_TOKEN=
